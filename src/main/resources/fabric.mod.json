{
	"schemaVersion": 1,
	"id": "zume",
	"version": "${mod_version}",
	"name": "${mod_name}",
	"description": "${mod_description}",
	"authors": [
		"${nolij}"
	],
	"contact": {
		"website": "${mod_url}",
		"repo": "${repo_url}",
		"issues": "${issue_url}"
	},
	"license": "OSL-3.0",
	"icon": "icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"dev.nolij.zume.FabricZumeBootstrapper"
		],
		"preLaunch": [
			"dev.nolij.zume.FabricZumeBootstrapper"
		],
		"modmenu": [
			"dev.nolij.zume.modern.integration.ZumeModMenuIntegration"
		],
		"stationapi:event_bus_client": [
			"dev.nolij.zume.primitive.event.KeyBindingRegistrar"
		]
	},
	"mixins": [
		"zume-modern.mixins.json",
		"zume-legacy.mixins.json",
		"zume-primitive.mixins.json"
	],
	"depends": {
		"fabricloader": [">=${fabric_version}", "${babric_version}"]
	}
}
