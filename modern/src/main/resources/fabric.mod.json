{
	"schemaVersion": 1,
	"id": "zume",
	"version": "${mod_version}",
	"name": "${mod_name}",
	"description": "${mod_description}",
	"authors": [
		"${nolij}"
	],
	"contact": {
		"website": "${mod_url}",
		"repo": "${repo_url}",
		"issues": "${issue_url}"
	},
	"license": "OSL-3.0",
	"icon": "icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"dev.nolij.zume.modern.ModernZume"
		],
		"modmenu": [
			"dev.nolij.zume.modern.integration.modmenu.ZumeModMenuIntegration"
		]
	},
	"mixins": [
		"zume-modern.mixins.json"
	],
	"depends": {
		"fabricloader": ">=${fabric_version}",
		"fabric-key-binding-api-v1": "*"
	}
}
